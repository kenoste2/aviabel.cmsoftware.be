	<wsdl:portType name="iigPortType">
		<wsdl:operation name="GetDemoCompanies">
			<wsdl:documentation>
				This method allows getting a list of demos companies which are free of charges.
			</wsdl:documentation>
			<wsdl:input message="bi:GetDemoCompaniesRequest" />
			<wsdl:output message="bi:GetDemoCompaniesResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="Search">
			<wsdl:documentation>
				This method allows to search the companies matching some criteria.
			</wsdl:documentation>
			<wsdl:input message="bi:SearchRequest" />
			<wsdl:output message="bi:SearchResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="GetAvailableProducts">
			<wsdl:documentation>
				This method allows getting a list of available products for a company. A product could be a report or other contents linked to the company.
			</wsdl:documentation>
			<wsdl:input message="bi:GetAvailableProductsRequest" />
			<wsdl:output message="bi:GetAvailableProductsResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="OrderReport">
			<wsdl:documentation>
				This method allows ordering a report on a company, previously found by the search method.
			</wsdl:documentation>
			<wsdl:input message="bi:OrderReportRequest" />
			<wsdl:output message="bi:OrderReportResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="GetAdditionalReportContents">
			<wsdl:documentation>
				This method allows getting additional contents for report (documents, ...)
			</wsdl:documentation>
			<wsdl:input message="bi:GetAdditionalReportContentsRequest" />
			<wsdl:output message="bi:GetAdditionalReportContentsResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="RequestUpdate">
			<wsdl:documentation>
				This method allows to order an update of the report if the response of "Order report" returned an UpdateFlag=M (update available on demand).
			</wsdl:documentation>
			<wsdl:input message="bi:RequestUpdateRequest" />
			<wsdl:output message="bi:RequestUpdateResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="CheckUpdates">
			<wsdl:documentation>
				This method allows to get the list of the companies which have been updated after a specific request or an automatic update. 
			</wsdl:documentation>
			<wsdl:input message="bi:CheckUpdatesRequest" />
			<wsdl:output message="bi:CheckUpdatesResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="GetUpdatedReport">
			<wsdl:documentation>
				This method allows to download the updated report for companies flagged as ready (R) by the "CheckUpdates" method.
			</wsdl:documentation>
			<wsdl:input message="bi:GetUpdatedReportRequest" />
			<wsdl:output message="bi:GetUpdatedReportResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="AddMonitoringReport">
			<wsdl:documentation>
				Request a monitoring on a specified company.
			</wsdl:documentation>
			<wsdl:input message="bi:AddMonitoringReportRequest" />
			<wsdl:output message="bi:AddMonitoringReportResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="RemoveMonitoringReport">
			<wsdl:documentation>
				Stop a monitoring on a specified company.
			</wsdl:documentation>
			<wsdl:input message="bi:RemoveMonitoringReportRequest" />
			<wsdl:output message="bi:RemoveMonitoringReportResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="CheckMonitoringReports">
			<wsdl:documentation>
				Get the list of all companies added to the monitoring which were updated based on specific dates.
			</wsdl:documentation>
			<wsdl:input message="bi:CheckMonitoringReportsRequest" />
			<wsdl:output message="bi:CheckMonitoringReportsResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="GetChangedReport">
			<wsdl:documentation>
				Get the report of the monitored company which has been changed.
			</wsdl:documentation>
			<wsdl:input message="bi:GetChangedReportRequest" />
			<wsdl:output message="bi:GetChangedReportResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="RequestOfflineReport">
			<wsdl:documentation>
				Request an offline report by specifying the IIG key of the company name/address/country of the company and the desired product. You will get an offlineID.
			</wsdl:documentation>
			<wsdl:input message="bi:RequestOfflineReportRequest" />
			<wsdl:output message="bi:RequestOfflineReportResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="CheckOfflineReportRequests">
			<wsdl:documentation>
				Check the status of your offline requests. 
			</wsdl:documentation>
			<wsdl:input message="bi:CheckOfflineReportRequestsRequest" />
			<wsdl:output message="bi:CheckOfflineReportRequestsResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
		<wsdl:operation name="GetRequestedOfflineReport">
			<wsdl:documentation>
				This method allows to download the offline report for companies flagged as ready (R) by the "CheckOfflineReportRequests" method.
			</wsdl:documentation>
			<wsdl:input message="bi:GetRequestedOfflineReportRequest" />
			<wsdl:output message="bi:GetRequestedOfflineReportResponse" />
			<wsdl:fault name="fault" message="bi:IIG_faultMsg" />
		</wsdl:operation>
	</wsdl:portType>
	
